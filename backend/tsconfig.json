{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "rootDir": "./src",
    "moduleResolution": "node10",
    "baseUrl": "./",
    "paths": {
      "*": [
        "src/*"
      ]
    },
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
