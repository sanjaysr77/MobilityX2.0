{
  "name": "mobility-backend",
  "version": "1.0.0",
  "description": "Backend API for MobilityX hackathon project",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "dev:all": "concurrently \"npm run dev\" \"cd ../frontend && npm run dev\"",
    "clean": "rm -rf dist"
  },
  "keywords": ["mobility", "transport", "api", "hackathon"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "concurrently": "^8.2.2"
  }
}